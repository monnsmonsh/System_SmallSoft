@model IEnumerable<SystemSmallSoft.Models.TblProveedorProducto>

@{
    ViewBag.Title = "Index";
    <!---->ViewBag.InventarioA = "active";
}
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="largeModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="largeModalLabel"></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div id="modal-content">
                    <!---->
                    Cargando...
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Cancel</button>
            </div>
        </div>
    </div>
</div>
<!-- end modal large -->

<div class="jumbotron" style="background-image: url(/Content/img/about-service.png); background-size: 100%;">
    <div class="container">
        <h1 class="display-3" _msthash="669669" _msttexthash="169715" style="color:dimgrey;">Inventario</h1>

    </div>
</div>

<a class="btn btn-primary btn-lg" href="~/ProveedorProducto/Create" role="button">Nuevo Entrada</a>


<table id="tblListEntradas" class="table table-striped table-bordered">
    <thead>
        <tr>
            <th scope="col">
                @Html.DisplayNameFor(model => model.TblProductos.SKU)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.TblProveedores.RazonSocial)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.Price)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.LastBuy)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.cantidad)
            </th>
            @*<th></th>*@
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.TblProductos.SKU)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TblProveedores.RazonSocial)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Price)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastBuy)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.cantidad)
                </td>
                @*<td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.ProveedorProductoID }) |
                    @Html.ActionLink("Details", "Details", new { id = item.ProveedorProductoID }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.ProveedorProductoID })
                </td>*@
            </tr>
        }
    </tbody>

</table>
